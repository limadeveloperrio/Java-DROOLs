package com.example.rules;

import com.motor.decision.dto.PersonDTO;
import com.motor.decision.dto.BenefitDTO;

rule "beneficio_15"
    salience 6
when
    $person: PersonDTO( educationLevel == "Ensino Superior", income < 6000.0 )
then
    BenefitDTO $b = new BenefitDTO();
    $b.setBenefit("Auxílio Educação");
    $b.setHasBenefit(true);
    $person.addLisBenefit($b);
    System.out.println("Benefício de Auxílio Educação para: " + $person.getName());
end
