package com.example.rules;

import com.motor.decision.dto.PersonDTO;
import com.motor.decision.dto.BenefitDTO;

rule "beneficio_35"
    salience 5
when
    $person: PersonDTO( age <= 25, isWorking == true, income >= 2000.0 )
then
    BenefitDTO $b = new BenefitDTO();
    $b.setBenefit("Auxílio para Jovens Trabalhadores");
    $b.setHasBenefit(true);
    $person.addLisBenefit($b);
    System.out.println("Benefício de Auxílio para Jovens Trabalhadores para: " + $person.getName());
end
