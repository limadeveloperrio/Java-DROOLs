package com.example.rules;

import com.motor.decision.dto.PersonDTO;
import com.motor.decision.dto.BenefitDTO;

rule "beneficio_37"
    salience 5
when
    $person: PersonDTO( age <= 30, income < 2000.0, hasChildren == true )
then
    BenefitDTO $b = new BenefitDTO();
    $b.setBenefit("Bolsa Família");
    $b.setHasBenefit(true);
    $person.addLisBenefit($b);
    System.out.println("Benefício de Bolsa Família para: " + $person.getName());
end